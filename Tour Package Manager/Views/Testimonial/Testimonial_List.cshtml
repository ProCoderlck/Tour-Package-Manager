
@{
    ViewBag.Title = "Testimonial_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/Testimonial_List.js?v=3.3.5"></script>
<div class="content">
    <div style="display:flex;align-items:center; justify-content:space-between" class="mb-2">
        <h4>Testimonial Management</h4>
        <a href="~/Testimonial/Add_Testimonial" class="btn btn-info btn-sm card-title" style="float:right">Add Testimonial</a>
    </div>
    <div class="card col-lg-12">
        <div class="card-header">
            <h>Testimonial List</h>
        </div>
        <div class="card-body card-block">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th scope="col" style="text-align:center">Action</th>
                        <th scope="col" style="text-align:center">Auto Id</th>
                        <th scope="col" style="text-align:center">Name</th>
                        <th scope="col" style="text-align:center">Profession</th>
                        <th scope="col" style="text-align:center">Image</th>

                    </tr>
                </thead>
                <tbody id="Data">
                </tbody>
            </table>
        </div>
    </div>
</div>



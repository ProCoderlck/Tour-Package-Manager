
@{
    ViewBag.Title = "User_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/UserList.js?v=4.5.9"></script>
    
    <div class="content">
        <div style="display:flex;align-items:center; justify-content:space-between" class="mb-2">
            <h4>User Management</h4>
            <a href="~/User/Add_User" class="btn btn-info btn-sm card-title" style="float:right">Add User</a>
        </div>
        <div class="card col-lg-12">
            <div class="card-header">
                <h>User List</h>
            </div>
            <div class="card-body card-block">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col"style="text-align:center">AutoId</th>
                            <th scope="col"style="text-align:center">User Name</th>
                            <th scope="col"style="text-align:center">Mobile No</th>
                            <th scope="col"style="text-align:center">Email</th>
                            <th scope="col"style="text-align:center">Status </th>
                            <th scope="col"style="text-align:center">Image </th>
                            <th scope="col"style="text-align:center">Action</th>
                        </tr>
                    </thead>
                    <tbody id="Data">
                       
                    </tbody>
                </table>
            </div>
        </div>
    </div>
